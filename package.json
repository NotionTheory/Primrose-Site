{
  "name": "primrose-site",
  "private": true,
  "description": "Primrose is a for Virtual Reality applications in Web browsers. It includes a basic syntax highlighting text editor.",
  "version": "0.28.18",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Sean T. McBeth <sean@seanmcbeth.com> (https://www.seanmcbeth.com)",
  "license": "GPL-3.0",
  "homepage": "http://www.primrosevr.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/capnmidnight/Primrose-Site.git"
  },
  "bugs": "https://github.com/capnmidnight/Primrose-Site/issues",
  "files": [
    "LICENSE.md",
    "README.md",
    "CONTRIBUTORS.md",
    "Primrose.js",
    "Primrose.min.js",
    "PrimroseDependencies.js",
    "PrimroseDependencies.min.js",
    "PrimroseDocumentation.js",
    "PrimroseDocumentation.min.js",
    "meeting/",
    "quickstart/",
    "src/",
    "doc/",
    "lib/"
  ],
  "scripts": {
    "start": "node server.js",
    "redev": "node server.js --mode=dev --path=..",
    "dev": "node server.js --mode=dev --path=.. --url=Primrose/meeting/?room=test",
    "demo": "node server.js --mode=dev --path=../Primrose --url=meeting/",
    "local": "node server.js --mode=localOnly --path=.. --url=Primrose/meeting/",
    "test": "node server.js --mode=dev --path=.. --url=replay-telemetry/test.html"
  },
  "keywords": [
    "virtual reality",
    "VR",
    "framework",
    "text",
    "editor",
    "HTML5",
    "canvas",
    "syntax",
    "highlighting"
  ],
  "dependencies": {
    "azure-storage": "^0.10.0",
    "mime": "^1.3.4",
    "opentok": "^2.4.0",
    "primrose": "^0.26.5",
    "socket.io": "^1.4.4"
  },
  "devDependencies": {
    "gulp-rename": "^1.2.2",
    "notiontheory-basic-build": "^2.6.0"
  }
}